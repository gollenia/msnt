{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "msn/downloads",
  "title": "Downloads",
  "category": "widgets",
  "keywords": [
    "icon",
    "symbol"
  ],
  "textdomain": "msn",
  "description": "Show a downloads list.",
  "attributes": {
    "downloads": {
      "type": "array",
      "default": []
    },
    "showDate": {
      "type": "boolean",
      "default": true
    }
  },
  "editorStyle": "wp-block-icon",
  "supports": {
    "html": false,
    "textAlign": true,
    "color": {
      "custom": true,
      "text": true
    }
  }
}